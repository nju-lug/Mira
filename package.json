{
  "name": "mira",
  "version": "0.2.0",
  "engines": {
    "node": ">=22.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vue-tsc --noEmit && vite build --base=/Mira/",
    "serve": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "vtsc": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "marked": "^4.2.5",
    "vue": "^3.5.16",
    "vue-gtag": "^3.5.1",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "~4.13.2",
    "@types/marked": "^4.0.8",
    "@types/node": "^22.15.29",
    "@vicons/ionicons5": "^0.13.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "eslint": "^9.28.0",
    "less": "^4.3.0",
    "naive-ui": "^2.41.0",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^6.0.1",
    "typescript": "^5.8.3",
    "vfonts": "^0.0.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
